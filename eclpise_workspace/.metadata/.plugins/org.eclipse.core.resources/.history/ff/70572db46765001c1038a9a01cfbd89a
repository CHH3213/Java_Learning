package pers.chh3213.classLearn;

public class transferTest {
	public static void main(String[] args) {
//		int a=10;
//		int b=10;
//		method(a,b);
//		System.out.println("a="+a);
//		System.out.println("b="+b);
		
		int[] arr = new int[10];
		System.out.println(arr);
		
	}
	public static void method(int a,int b) {
		a=a*10;
		b=b*10;
		System.out.println("a="+a);
		System.out.println("b="+b);
		System.exit(0);
	}
}
