package pers.chh3213.sort;
import java.util.Scanner;
public class BubbleSort {
	public static void main(String[] args) {
		int[] arr = {5,4,1,966,2,3,56,89,12};
		for (int i : arr) {
			System.out.print(i);
		}
		for(int i=0;i<arr.length-1;i++) {
				if(arr[i]>arr[i+1]) {
					int temp = arr[i];
					arr[i]= arr[i+1];
					arr[i+1] = temp;
				}
			}
		System.out.println();
		for (int i : arr) {
			System.out.print(i);
		}
		}

}
